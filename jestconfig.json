{
    "transform": {
        "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "\\.test\\.ts$",
    "moduleFileExtensions": ["ts", "js"],
    "moduleDirectories": ["node_modules", "src"],
    "collectCoverage": true,
    "verbose": true,
    "globals": {
        "ts-jest": {
            "diagnostics": true
        }
    },
    "modulePaths": ["<rootDir>/src/"]
}
